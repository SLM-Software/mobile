{
    "name": "com.spotlight.retail",
    "version": "1.0.7",
    "displayName": "Spotlightmart",
    "cordova": {
        "platforms": [
            "browser",
            "ios"
        ],
        "plugins": {
            "cordova-plugin-actionsheet": {},
            "cordova-plugin-camera": {
                "CAMERA_USAGE_DESCRIPTION": "Spotlightmart",
                "PHOTOLIBRARY_USAGE_DESCRIPTION": "Spotlightmart"
            },
            "cordova-plugin-console": {},
            "cordova-plugin-dialogs": {},
            "cordova-plugin-file": {},
            "cordova-plugin-inappbrowser": {},
